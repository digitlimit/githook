{
    "name": "digitlimit/githook",
    "description": "Laravel Git Webhook: A Laravel package for handling GitHub webhooks. Listen for GitHub webhook events from your Laravel application.",
    "license": "MIT",
    "authors": [
        {
            "name": "Emeka Mbah",
            "email": "frankemeks77@yahoo.com",
            "homepage": "https://emekambah.medium.com"
        }
    ],
    "homepage": "https://github.com/digitlimit/githook",
    "keywords": [
        "laravel",
        "github",
        "webhooks",
        "laravel-package",
        "github-webhooks",
        "webhook-handler",
        "php",
        "open-source",
        "developer-tools",
        "git-integration",
        "github-integration",
        "laravel-webhooks",
        "webhook-events",
        "event-listeners",
        "laravel-git",
        "git-automation",
        "github-actions",
        "continuous-integration",
        "ci-cd",
        "devops"
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^7.20|^8.19|^9.0|^10.0|^11.0",
        "illuminate/support": "^7.20|^8.19|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "laravel/framework": "^7.20|^8.19|^9.0|^10.0|^11.0",
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.5",
        "nunomaduro/collision": "^7.5",
        "pestphp/pest": "^2.5",
        "laravel/pint": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Digitlimit\\Githook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digitlimit\\Githook\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digitlimit\\Githook\\GithookServiceProvider"
            ],
            "aliases": {
                "Githook": "Digitlimit\\Githook\\Facades\\Githook"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
